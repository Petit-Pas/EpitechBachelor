[13]
NAME="redirection >"
SETUP=""
CLEAN=""
TESTS=
  echo 'ls > .testlolmdr'
  echo 'echo bite'
  echo '/bin/grep mysh .testlolmdr >> .testlolmdr'
  echo '/bin/grep sh .testlolmdr >> .testlolmdr'
  echo 'cat .testlolmdr'
  echo 'rm .testlolmdr'
[13-END]

